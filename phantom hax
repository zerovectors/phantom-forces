    main.VelCap = 5;
    main.JumpBoostAmt = 0.1;
    if main.espsettings.CHAMSLength > 2048 then
        main.espsettings.CHAMSLength = 2048
    end;
    if main.espsettings.TracersLength > 2048 then
        main.espsettings.TracersLength = 2048
